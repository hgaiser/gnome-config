# vim: ft=dosini

[org/gnome/desktop/background]
color-shading-type='solid'
picture-options='zoom'
picture-uri='file:///usr/share/backgrounds/gnome/LightBulb.jpg'
primary-color='#ffffff'
secondary-color='#000000'

[org/gnome/desktop/calendar]
show-weekdate=true

[org/gnome/desktop/input-sources]
xkb-options=['lv3:ralt_switch', 'compose:caps']

[org/gnome/desktop/interface]
clock-show-seconds=true
enable-hot-corners=false
gtk-theme='Adwaita-dark'
icon-theme='Numix-Circle'
show-battery-percentage=true

[org/gnome/desktop/peripherals/mouse]
accel-profile='flat'
natural-scroll=false

[org/gnome/desktop/peripherals/touchpad]
speed=0.55160142348754437
tap-to-click=true

[org/gnome/desktop/sound]
event-sounds=false

[org/gnome/mutter]
experimental-features=['scale-monitor-framebuffer']
overlay-key='Super_R'
workspaces-only-on-primary=false

[org/gnome/nautilus/preferences]
default-folder-viewer='list-view'
search-view='list-view'

[org/gnome/settings-daemon/plugins/media-keys]
area-screenshot-clip=['<Shift><Super>s']
custom-keybindings=['/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom0/']

[org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom0]
binding='<Super>t'
command='alacritty'
name='Alacritty'

[org/gnome/settings-daemon/plugins/power]
sleep-inactive-ac-type='nothing'

[org/gnome/shell]
favorite-apps=['Alacritty.desktop', 'firefox.desktop', 'thunderbird.desktop', 'slack.desktop', 'teams.desktop', 'whatsdesk.desktop', 'signal-desktop.desktop', 'telegramdesktop.desktop', 'steam-native.desktop', 'spotify.desktop']

[org/gnome/shell/app-switcher]
current-workspace-only=true

[org/gnome/shell/extensions/arch-update]
update-cmd="tilix --maximize -e 'sh -c  \"sudo pacman -Syu ; echo Done - Press enter to exit; read\" '"

[org/gnome/shell/extensions/dash-to-panel]
animate-app-switch=true
animate-appicon-hover=false
animate-appicon-hover-animation-convexity={'RIPPLE': 2.0, 'PLANK': 1.0, 'SIMPLE': 0.0}
animate-appicon-hover-animation-extent={'RIPPLE': 4, 'PLANK': 4, 'SIMPLE': 1}
animate-appicon-hover-animation-type='SIMPLE'
animate-window-launch=true
appicon-margin=0
appicon-padding=4
available-monitors=[1, 0]
click-action='CYCLE'
dot-position='TOP'
dot-style-focused='METRO'
dot-style-unfocused='DOTS'
group-apps=true
hide-overview-on-startup=true
hot-keys=false
hotkeys-overlay-combo='TEMPORARILY'
intellihide=false
intellihide-animation-time=10
intellihide-behaviour='ALL_WINDOWS'
intellihide-hide-from-windows=true
isolate-workspaces=false
leftbox-padding=-1
leftbox-size=0
multi-monitors=true
overview-click-to-exit=true
panel-anchors='{"0":"START"}'
panel-element-positions='{"0":[{"element":"activitiesButton","visible":false,"position":"stackedTL"},{"element":"leftBox","visible":true,"position":"stackedTL"},{"element":"showAppsButton","visible":false,"position":"stackedTL"},{"element":"taskbar","visible":true,"position":"stackedTL"},{"element":"centerBox","visible":true,"position":"stackedTL"},{"element":"dateMenu","visible":true,"position":"centerMonitor"},{"element":"rightBox","visible":true,"position":"stackedBR"},{"element":"systemMenu","visible":true,"position":"stackedBR"},{"element":"desktopButton","visible":false,"position":"stackedBR"}],"1":[{"element":"activitiesButton","visible":false,"position":"stackedTL"},{"element":"leftBox","visible":true,"position":"stackedTL"},{"element":"showAppsButton","visible":false,"position":"stackedTL"},{"element":"taskbar","visible":true,"position":"stackedTL"},{"element":"centerBox","visible":true,"position":"stackedTL"},{"element":"dateMenu","visible":true,"position":"centerMonitor"},{"element":"rightBox","visible":true,"position":"stackedBR"},{"element":"systemMenu","visible":true,"position":"stackedBR"},{"element":"desktopButton","visible":false,"position":"stackedBR"}]}'
panel-element-positions-monitors-sync=true
panel-lengths='{"0":100,"1":100}'
panel-positions='{"0":"TOP","1":"TOP"}'
panel-sizes='{"0":27,"1":27}'
primary-monitor=1
secondarymenu-contains-showdetails=true
show-appmenu=false
show-favorites=true
show-running-apps=true
show-tooltip=false
show-window-previews=false
status-icon-padding=4
stockgs-keep-dash=false
stockgs-keep-top-panel=false
stockgs-panelbtn-click-only=false
trans-bg-color='#000000'
trans-dynamic-anim-target=1.0
trans-dynamic-anim-time=300
trans-dynamic-behavior='ALL_WINDOWS'
trans-dynamic-distance=20
trans-panel-opacity=0.59999999999999998
trans-use-custom-bg=false
trans-use-custom-opacity=false
trans-use-dynamic-opacity=false
tray-padding=4
tray-size=0
window-preview-title-position='TOP'

[org/gnome/shell/extensions/display-brightness-ddcutil]
show-all-slider=true
show-value-label=true

[org/gnome/shell/extensions/extension-list]
delete-button=true
hide-disabled=false
unpin-button=false

[org/gnome/shell/extensions/gestureImprovements]
touchpad-speed-scale=1.0

[org/gnome/shell/extensions/sound-output-device-chooser]
expand-volume-menu=false
hide-on-single-device=true
icon-theme='monochrome'

[org/gnome/shell/extensions/unite]
extend-left-box=true
hide-activities-button='always'
hide-dropdown-arrows=true
restrict-to-primary-screen=false
show-desktop-name=false
show-legacy-tray=false
window-buttons-theme='default-dark'

[org/gnome/desktop/wm/keybindings]
close=['<Alt>F4', '<Super>q']

[org/gnome/shell/keybindings]
toggle-overview=['<Super>d']
