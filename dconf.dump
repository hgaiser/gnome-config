[com/gexperts/Tilix]
control-scroll-zoom=true
enable-wide-handle=false
new-instance-mode='new-window'
new-window-inherit-state=true
prompt-on-close=true
quake-specific-monitor=0
sidebar-on-right=true
terminal-title-show-when-single=false
terminal-title-style='none'
theme-variant='dark'
unsafe-paste-alert=false
use-overlay-scrollbar=true
use-tabs=true
warn-vte-config-issue=false
window-save-state=true
window-state=43908
window-style='borderless'

[com/gexperts/Tilix/keybindings]
session-add-down='<Shift><Super>Return'
session-add-right='<Super>Return'

[org/gnome/desktop/input-sources]
xkb-options=['compose:caps']

[org/gnome/desktop/interface]
clock-show-date=true
clock-show-seconds=true
enable-animations=false
gtk-im-module='gtk-im-context-simple'
gtk-theme='Arc-Darker'
icon-theme='Numix-Circle'

[org/gnome/desktop/peripherals/touchpad]
tap-to-click=true
two-finger-scrolling-enabled=true

[org/gnome/desktop/wm/keybindings]
close=['<Alt>F4', '<Super>q']
move-to-workspace-1=['<Shift><Super>1']
move-to-workspace-10=['<Shift><Super>0']
move-to-workspace-2=['<Shift><Super>2']
move-to-workspace-3=['<Shift><Super>3']
move-to-workspace-4=['<Shift><Super>4']
move-to-workspace-5=['<Shift><Super>5']
move-to-workspace-6=['<Shift><Super>6']
move-to-workspace-7=['<Shift><Super>7']
move-to-workspace-8=['<Shift><Super>8']
move-to-workspace-9=['<Shift><Super>9']
panel-main-menu=['<Super>d']
switch-applications=['<Super>Tab']
switch-to-workspace-1=['<Super>1']
switch-to-workspace-10=['<Super>0']
switch-to-workspace-2=['<Super>2']
switch-to-workspace-3=['<Super>3']
switch-to-workspace-4=['<Super>4']
switch-to-workspace-5=['<Super>5']
switch-to-workspace-6=['<Super>6']
switch-to-workspace-7=['<Super>7']
switch-to-workspace-8=['<Super>8']
switch-to-workspace-9=['<Super>9']
switch-windows=['<Alt>Tab']

[org/gnome/desktop/wm/preferences]
num-workspaces=10
workspace-names=['dev', 'web', 'mail', 'chat', 'doc', 'misc', 'viz', '8', '9', '0']

[org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom0]
binding='<Super>t'
command='tilix'
name='Tilix'

[org/gnome/shell/extensions/topicons]
tray-pos='right'

[org/gnome/shell/extensions/unite]
extend-left-box=false
hide-activities-button='always'
hide-dropdown-arrows=true
restrict-to-primary-screen=false
show-desktop-name=true
show-legacy-tray=false

[org/gnome/shell/extensions/workspacebuttons]
active-color='#73a0d1'
buttons-position-change=false
buttons-position-index=1
click-to-activate=true
empty-color='#222222'
empty-workspace-style=true
hover-color='#ffffff'
inactive-color='#676767'
urgent-color='#e70909'
urgent-workspace-style=true
workspace-label-indicator=false
workspace-label-indicators=['', '', '']
workspace-label-name=true
workspace-label-number=false

[org/gnome/shell/keybindings]
switch-to-application-1=@as []
switch-to-application-2=@as []
switch-to-application-3=@as []
switch-to-application-4=@as []
switch-to-application-5=@as []
switch-to-application-6=@as []
switch-to-application-7=@as []
switch-to-application-8=@as []
switch-to-application-9=@as []

[org/gnome/shell/overrides]
